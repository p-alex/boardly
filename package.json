{
  "name": "boardly",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "packageManager": "npm@11.8.0",
  "license": "ISC",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "docker:dev": "docker compose -f docker-compose.dev.yml up",
    "docker:build": "docker compose -f docker-compose.dev.yml up --build",
    "docker:dev-stop": "docker compose -f docker-compose.dev.yml stop",
    "docker:prod": "docker compose -f docker-compose.prod.yml up --build",
    "docker:prod-stop": "docker compose -f docker-compose.prod.yml stop",
    "docker:stop": "npm run docker:prod-stop && npm run docker:dev-stop"
  },
  "devDependencies": {
    "turbo": "^2.8.1"
  }
}
